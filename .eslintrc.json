{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": 0,
    "no-use-before-define": ["error", "nofunc"],
    "no-param-reassign": "off"
  }
}
